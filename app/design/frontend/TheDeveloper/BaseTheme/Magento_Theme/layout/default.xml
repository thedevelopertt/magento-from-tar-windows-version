<page layout="2columns" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_width" xsi:type="number">48</argument>
                <argument name="logo_height" xsi:type="number">48</argument>
                <argument name="logo_alt" xsi:type="string">The Developer Logo</argument>
            </arguments>
        </referenceBlock>

        <referenceBlock name="report.bugs" remove="true"/>

        <referenceContainer name="footer">
            <block class="Magento\Framework\View\Element\Template" template="html/tothedeveloper.phtml"/>
        </referenceContainer>

    </body>
</page>
